#container
{
	width: 1100px;
	margin: 150px auto;
	background-color: #EEE;
	overflow: hidden;
	padding: 15px;
	opacity: 0.88;
}

#main
{
	width: 790px;
	float: left;
}

#sidebar
{
	width: 300px;
	float: left;
}

ul
{
	list-style-type: none;
	margin: 10px;
	padding: 20px;
	margin-left: 50px;
	padding-left: 100px;
}

li
{
	display: inline;
	float: left;
}

#mylist a 
{
	padding: 12px 18px;
	color: #f5f5f5;
	background-color: #290A00;
}

#mylist a:hover, #mylist a:active, #mylist a:focus 
{
	background-color: #006B24 !important;
	text-decoration: none;
	color: #ffffff !important;	
}

a
{
	color: #48577D;
	text-decoration: none; 
}

#TableHead1
{
	color: #FFFFFF;
	font-size: 18px;
}

#TableHead2
{
	color: #FFFFFF;
	font-size: 18px;
}

#TotalPrice
{
	font-size: 18px;
}

a:hover 
{
	text-decoration: underline;
}

h1, h2
{
	margin-bottom: 15px;
}

h1
{
	font-size: 18px;
}

h2
{
	font-size: 16px;
}

#main table
{
	width: 680px;
}

#main table th
{
	padding: 10px;
	background-color: #48577D;
	color: #FFF;
	text-align: left;
}

#main table td
{
	padding: 5px;
}

#main table tr
{
	background-color: #d3dcf2; 
}

a#register
{
    position: relative;
    bottom: 35px;
    left: 80px;
}

h1#titlu
{
	letter-spacing: 2px;
	font-size: 100px;
    position: middle;
}

img.circuit
{
    position:relative;
    left:1500px;
    top:-400px;
    height:80px;
    width: 80px;
}

